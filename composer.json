{
    "name": "zored/config-admin",
    "description": "ZF2 module for easy administration of application settings",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "zend",
        "module"
    ],
    "homepage": "https://github.com/cgmartin/CgmConfigAdmin",
    "authors": [
        {
            "name": "Christopher Martin",
            "email": "cgmartin@gmail.com",
            "homepage": "http://cgmartin.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "zendframework/zendframework": ">=2.0.0",
        "zf-commons/zfc-base": ">=0.0.1"
    },
    "suggest": {
        "bjyoungblood/bjy-authorize": ">=1.1.1",
        "zf-commons/zfc-rbac": ">=0.1.1",
        "zf-commons/zfc-user": ">=0.0.1",
        "zf-commons/zfc-admin": ">=0.1.0"
    },
    "autoload": {
        "psr-0": {
            "CgmConfigAdmin": "src/"
        },
        "classmap": [
            "./"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10"
    }
}
